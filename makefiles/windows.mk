WIN32_PATH_BIN := $(subst /,\,$(PATH_BIN))
WIN32_PATH_OBJ := $(subst /,\,$(PATH_OBJ))
WIN32_PATH_OBJ_X86_LINUX := $(subst /,\,$(PATH_OBJ_X86_LINUX))
WIN32_PATH_OBJ_X86_WINDOWS := $(subst /,\,$(PATH_OBJ_X86_WINDOWS))
WIN32_PATH_OBJ_X86_64_LINUX := $(subst /,\,$(PATH_OBJ_X86_64_LINUX))
WIN32_PATH_OBJ_X86_64_WINDOWS := $(subst /,\,$(PATH_OBJ_X86_64_WINDOWS))

mkdir:
	md $(WIN32_PATH_OBJ_X86_LINUX)
	md $(WIN32_PATH_OBJ_X86_WINDOWS)
	md $(WIN32_PATH_OBJ_X86_64_LINUX)
	md $(WIN32_PATH_OBJ_X86_64_WINDOWS)
	
clean:
	rm $(WIN32_PATH_BIN)*.*
	rm $(WIN32_PATH_OBJ)*.*
	rm $(WIN32_PATH_OBJ_X86_LINUX)*.*
	rm $(WIN32_PATH_OBJ_X86_WINDOWS)*.*
	rm $(WIN32_PATH_OBJ_X86_64_LINUX)*.*
	rm $(WIN32_PATH_OBJ_X86_64_WINDOWS)*.*